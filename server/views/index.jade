      button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-collapse')
        span(class='icon-bar')
        span(class='icon-bar')
        span(class='icon-bar')
      a.navbar-brand(href=(title_link ? title_link : '#'))= title_text
    .collapse.navbar-collapse
      ul.nav.navbar-nav
        li.timing
          a(href='javascript:category("timing")') Timing
        li.sizing
