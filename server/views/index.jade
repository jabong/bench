.navbar.navbar-fixed-top
  .navbar-inner
    .container
      a.brand(href="htt://github.com/jmervine/bench") Bench UI
      .nav-collapse
        ul.nav
          li.timing
            a(href="javascript:category('timing')") Timing
          li.sizing
            a(href="javascript:category('sizing')") Sizing
          li.requests
            a(href="javascript:category('requests')") Requests
          li.counts
            a(href="javascript:category('counts')") Counts

.container-fluid(style="margin-top: 75px")
  .row-fluid
    .span2
      .well.sidebar-nav
        select#keyselect.form-control(multiple="multiple" style="margin: 0; width:100%")
    .span10
      .row-fluid
        .span2
          ul.pager(style="margin-left:50px")
            li.previous.first
              a(href="javascript:navigate('first')") first
            li.earlier
              a(href="javascript:navigate('earlier')") earlier
        .span8
          .form-group(style="margin: 20px auto; heigh: 50px; width: 600px")
            form
              b.form-label(style="font-size: 130%") Page:
              select#urlselect.form-control(style="width: 300px; font-size: 130%; margin-right: 25px; margin-left: 25px")
                option Select Page
              b.form-label(style="font-size: 130%") Limit:
              select#limitselect.form-control(style="width: 100px; font-size: 130%; margin-left: 25px")
                option(value="10") 10
                option(value="25") 25
                option(value="50") 50

        .span2
          ul.pager(style="margin-right:50px")
            li.later
              a(href="javascript:navigate('later')") later
            li.next.last
              a(href="javascript:navigate('last')") last

      .row-fluid
        .span12
          #highcharts(style="width:100%; height:400px;")
